<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated gpx -->
      <package
            uri="http://www.topografix.com/GPX/1/0"
            class="egeo.gpx.GpxPackage"
            genModel="model/gpx.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated gpx -->
      <parser
            type="gpx"
            class="egeo.gpx.util.GpxResourceFactoryImpl"/>
   </extension>
   
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated groundspeak -->
      <package
            uri="http://www.groundspeak.com/cache/1/0/1"
            class="egeo.gpx.groundspeak.GroundspeakPackage"
            genModel="model/groundspeak.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated groundspeak -->
      <parser
            type="groundspeak"
            class="egeo.gpx.groundspeak.util.GroundspeakResourceFactoryImpl"/>
   </extension>
</plugin>
